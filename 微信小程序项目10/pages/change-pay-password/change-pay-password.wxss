@import "../component/component.wxss";/*6位数输入框*/
/* pages/withdraw-deposit/withdraw-deposit.wxss */
.container{background-color: #f0eff5;height: 100%;box-sizing: border-box;}
/*修改支付密码  */
.list{padding: 0 27.083rpx;background-color: #ffffff;line-height: 114.583rpx;position: relative;text-align: center}
.lists .list-header{border-bottom:1rpx solid #dcdcdc;background-color: #f0eff5;}
.lists{background-color: white;}
.lists .content{background-color: white;padding-top: 75rpx;padding-bottom: 75rpx;}
.lists .content .list{line-height: 110rpx;height: 240rpx}
.lists .list-header text{font-size: 31.25rpx;color: #000;}
.list .payPassword{font-size: 0;}
.old.content{padding-top: 100rpx;height: auto;padding-bottom: 100rpx;}
.old.content .list{height: auto;}
.prompt{color:#fff;padding:10rpx 40rpx;font-size: 24rpx; border-radius: 20rpx;background-color: #626262;position: fixed;bottom: 200rpx;left:50%;transform: translateX(-50%);display: block;}
.prompt.hide{display: none;}
